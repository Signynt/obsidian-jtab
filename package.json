{
	"name": "obsidian-jtab",
	"version": "0.0.1",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "run-s dist && node esbuild.config.mjs",
		"clean": "rm -rf dist",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"sass": "sass --no-source-map src/assets/scss/styles.scss dist/styles.css",
		"meta": "mkdir -p dist && cp manifest.json versions.json dist",
		"dist": "run-s clean build sass meta"
	},
	"keywords": [],
	"author": "davfive",
	"license": "MIT",
	"devDependencies": {
		"@types/jquery": "3.5.14",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "5.2.0",
		"@typescript-eslint/parser": "5.2.0",
		"builtin-modules": "3.2.0",
		"esbuild": "0.13.12",
		"jquery": "3.6.0",
		"npm-run-all": "4.1.5",
		"obsidian": "0.14.4",
		"raphael": "2.3.0",
		"sass": "1.50.0",
		"tslib": "2.3.1",
		"typescript": "4.4.4"
	}
}
